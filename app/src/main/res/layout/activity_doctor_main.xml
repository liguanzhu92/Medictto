<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/activity_doctor_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.svs.myprojects.mymedicalrecords.doctor.DoctorMainActivity">
    <android.support.design.widget.BottomNavigationView
            android:id="@+id/bottom_nav"
            android:layout_alignParentBottom="true"
            app:menu="@menu/bottom_doc_nav"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@color/teal100"
            app:popupTheme="@style/AppTheme.PopupOverlay"/>
    <RelativeLayout
            android:layout_above="@id/bottom_nav"
            android:id="@+id/doctor_main_container"
            android:layout_width="match_parent" android:layout_height="match_parent"
            android:layout_alignParentStart="true"/>

</RelativeLayout>
